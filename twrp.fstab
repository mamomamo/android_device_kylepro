/boot            emmc    /dev/block/mmcblk0p5
/system          ext4    /dev/block/mmcblk0p17
/data            ext4    /dev/block/mmcblk0p19   length=-16384
/cache           ext4    /dev/block/mmcblk0p16
/recovery        emmc    /dev/block/mmcblk0p6
/efs             ext4    /dev/block/mmcblk0p15                         flags=display="EFS";backup=1
/external_sd     vfat    /dev/block/mmcblk1p1    /dev/block/mmcblk1    flags=display="Micro SDcard";storage;wipeingui;removable
/preload         ext4    /dev/block/mmcblk0p18                         flags=display="Preload";wipeingui;backup=1